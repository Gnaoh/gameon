<div class="container">
	Welcome, <%= @user.first_name %> <%= @user.last_name%><br>

	Age: <%= @user.age%><br>
	Gender: <%= @user.gender%><br>

</div>
<div>
	<h1> My Activities </h1>
  <% @user.events.each do |event| %>
  	<div class='list-group'>
  	 <p> <%= link_to event.name, event_path(event)%> Filled: <%= event.users.count + event.current_headcount%>/<%= event.max_headcount%></p> <br>
  	</div>
</div>
<br>

<% end %>